# 服務配置
PORT=4000

# 數據庫配置 (這些值將在 Render 部署時替換)
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=travel_plan_db

# JWT 配置
JWT_SECRET=your_secret_key_change_in_production
